{
	"name": "simple-jupyter-notebook",
	"displayName": "Simple Jupyter Notebook",
	"version": "0.0.1",
	"private": false,
	"description": "Starter extension for developing debug adapters for VS Code.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"updatetypes": "cd src/types && vscode-dts dev && vscode-dts master",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"breakpoints": [
			{
				"language": "python"
			}
		],
		"commands": [
			{
				"command": "simple-jupyter-notebook.toggleDebugging",
				"icon": "$(bug)",
				"title": "Toggle Notebook Debugging"
			},
			{
				"command": "simple-jupyter-notebook.restart-kernel",
				"icon": "$(refresh)",
				"title": "Restart Notebook Kernel"
			}
		],
		"configuration": [
			{
				"properties": {
					"simple-jupyter.searchPaths": {
						"default": [],
						"description": "List of folders to search to discover Jupyter kernels",
						"items": {
							"type": "string"
						},
						"type": "array"
					}
				},
				"title": "Simple Jupyter Kernels"
			}
		],
		"debuggers": [
			{
				"label": "xeus Debug",
				"type": "xeus"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "simple-jupyter-notebook.toggleDebugging",
					"group": "navigation@1",
					"when": "notebookEditorFocused"
				},
				{
					"command": "simple-jupyter-notebook.restart-kernel",
					"group": "navigation@2",
					"when": "notebookEditorFocused"
				}
			]
		},
		"notebookProvider": [
			{
				"displayName": "Jupyter",
				"selector": [
					{
						"excludeFileNamePattern": "*.test.ipynb",
						"filenamePattern": "*.ipynb"
					}
				],
				"viewType": "jupyter"
			}
		]
	},
	"activationEvents": [
		"onNotebookEditor:jupyter",
		"onNotebook:jupyter",
		"onCommand:simple-jupyter-notebook.restart-kernel",
		"onDebug:xeus"
	],
	"dependencies": {
		"@nteract/messaging": "^7.0.10",
		"get-port": "^5.1.1",
		"rxjs": "^6.6.3",
		"split2": "^3.2.2",
		"vscode-debugprotocol": "^1.41.0",
		"zeromq": "^6.0.0-beta.6"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/split2": "^2.1.6",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^3.4.3",
		"typescript-esbuild": "latest",
		"vscode-dts": "^0.3.1",
		"vscode-test": "^1.4.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"enableProposedApi": true
}
